{
  "name": "ava-fixture",
  "version": "0.0.0",
  "description": "Write fixture tests with ava",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "typings.json",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -d -p tsconfig.build.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "rimraf dist && tsc && ava \"dist/**/*.spec.js\"",
    "watch": "node scripts/watch.js",
    "verify": "npm run build && npm -s run test+lint",
    "build+test": "npm run build && npm test",
    "test+lint": "npm test && npm run lint",
    "preversion": "npm run verify",
    "prepublish": "typings install"
  },
  "license": "MIT",
  "keywords": [
    "ava",
    "fixture"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/unional/ava-fixture.git"
  },
  "bugs": {
    "url": "https://github.com/unional/ava-fixture/issues"
  },
  "homepage": "https://github.com/unional/ava-fixture",
  "devDependencies": {
    "ava": "^0.15.2",
    "onchange": "^2.5.0",
    "rimraf": "^2.5.3",
    "tap-spec": "^4.1.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0",
    "tslint-config-unional": "^0.3.0",
    "typescript": "^2.1.0-dev.20160712",
    "typings": "^1.3.1"
  },
  "dependencies": {
    "ava": "^0.15.2"
  }
}
